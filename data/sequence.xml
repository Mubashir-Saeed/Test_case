<odoo>
    <data noupdate="1">
        <record model="ir.sequence" id="seq_hospital_patient">
            <field name="name">Patient Sequence</field>
            <field name="code">hospital.patient.sequence</field>
            <field name="prefix">LP</field>
            <field name="padding">4</field>
            <field name="company_id" eval="False"/>
        </record>
    </data>
    <data noupdate="1">
        <record model="ir.sequence" id="seq_hospital_test">
            <field name="name">Test Sequence</field>
            <field name="code">hospital.test.sequence</field>
            <field name="prefix">LT</field>
            <field name="padding">4</field>
            <field name="company_id" eval="False"/>
        </record>
    </data>
    <data noupdate="1">
        <record model="ir.sequence" id="seq_hospital_report">
            <field name="name">Report Sequence</field>
            <field name="code">hospital.report.sequence</field>
            <field name="prefix">LR</field>
            <field name="padding">4</field>
            <field name="company_id" eval="False"/>
        </record>
    </data>
</odoo>